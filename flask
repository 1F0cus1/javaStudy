from flask import Flask
from flask import request
app = Flask(__name__)

@app.route('/')
def hello_world():
    return 'Hello,World!'

@app.route('/name',methods = ['GET','POST'])
def get_name():
    if request.method == 'POST':
        return 'cyf'
    else:
        return '陈宇帆'

@app.route('/age')
def get_age():
    return '23'

@app.route('/userProfile')
def get_userProfile():
    return dict(name = 'cyf',age = 23)
